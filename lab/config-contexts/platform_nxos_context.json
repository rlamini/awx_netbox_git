{
  "platform": {
    "os": "NX-OS",
    "vendor": "Cisco Systems"
  },
  "features": {
    "required": [
      "interface-vlan",
      "lacp",
      "lldp",
      "ssh"
    ]
  },
  "spanning_tree": {
    "mode": "rapid-pvst",
    "priority": 4096
  },
  "vtp": {
    "mode": "transparent"
  },
  "management": {
    "ssh_version": 2,
    "enable_scp": true
  }
}
